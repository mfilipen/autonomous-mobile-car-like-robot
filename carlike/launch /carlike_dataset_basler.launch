<?xml version="1.0"?>
<launch>
	<param name="use_sim_time" value="true" />

	<node pkg="rosbag" type="play" name="rosbag_play"
          args="--clock --pause -r 0.6 '$(find carlike)/datasets/2018-04-07-22-53-25.bag'"/>

	<node name="republish_pylon" type="republish" pkg="image_transport" output="screen"
		  args="compressed in:=/pylon_camera_node/image_raw/ raw out:=/camera/image_raw" />

</launch>
