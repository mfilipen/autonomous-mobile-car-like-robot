<?xml version="1.0"?>
<launch>
	<param name="use_sim_time" value="true" />


        <node pkg="rosbag" type="play" name="rosbag_play"
              args="--clock --pause -r 0.5 '$(find carlike)/datasets/2018-04-07-20-35-57.bag'"/>

        <node name="republish_left" type="republish" pkg="image_transport" output="screen"
              args="compressed in:=/zed/left/image_raw_color/ raw out:=/camera/left/image_raw" />

        <node name="republish_right" type="republish" pkg="image_transport" output="screen"
              args="compressed in:=/zed/right/image_raw_color/ raw out:=/camera/right/image_raw" />

</launch>
